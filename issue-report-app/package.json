{
  "name": "github-ai",
  "version": "1.0.0",
  "description": "GitHub API client for fetching issue and project board details",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "ui:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "ts-node src/server.ts",
    "client:dev": "cd ui && npm start",
    "ui:build": "cd ui && npm run build",
    "ui:install": "cd ui && npm install"
  },
  "keywords": ["github", "api", "issues", "projects", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "concurrently": "^8.2.2"
  }
} 